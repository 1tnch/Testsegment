 digitalWrite(3, LOW);
 digitalWrite(2, HIGH);
 digitalWrite(1, HIGH);
 digitalWrite(4, HIGH);
 digitalWrite(5 , HIGH);
 delay(time_d2);
 
  digitalWrite(5, LOW);
  digitalWrite(6, HIGH);
  digitalWrite(7, LOW);
  digitalWrite(8, LOW);
  digitalWrite(9, LOW);  // 4th 0
  digitalWrite(10, LOW);
  digitalWrite(11, LOW);
  digitalWrite(12, LOW);
  digitalWrite(13, LOW);

 digitalWrite(3, HIGH);
 digitalWrite(2, HIGH);
 digitalWrite(1, HIGH);
 digitalWrite(4, LOW);
 digitalWrite(5 , HIGH);
 delay(time_d2);
  }
  for (int i=0; i<4; i++){
  digitalWrite(5, LOW);
  digitalWrite(6, HIGH);
  digitalWrite(7, LOW);
  digitalWrite(8, LOW);
  digitalWrite(9, LOW);  // 5th 0
  digitalWrite(10, LOW);
  digitalWrite(11, LOW);
  digitalWrite(12, LOW);
  digitalWrite(13, LOW);

 digitalWrite(3, HIGH);
 digitalWrite(2, HIGH);
 digitalWrite(1, HIGH);
 digitalWrite(4, HIGH);
 digitalWrite(5 , LOW);
 delay(time_d3);
  }
}

 for (int i=0; i<1; i++){

 digitalWrite(5, HIGH);
  digitalWrite(6, LOW);
  digitalWrite(7, LOW);
  digitalWrite(8, LOW);
  digitalWrite(9, LOW);  // 1st 2
  digitalWrite(10, HIGH);
  digitalWrite(11, LOW);
  digitalWrite(12, LOW);
  digitalWrite(13, LOW);

 
  digitalWrite(1, HIGH);
  digitalWrite(2, LOW);
